<div class="home-join-wrapper d-flex align-items-center ">
    <div class="container">
      <div class="row">
        <div class="col-md-3 offset-md-1">
           <div class="join-image-wrapper">
            <img data-sizes="auto"
                data-src="{{ section.settings.img | img_url:'' }}" class="lazyload" loading="lazy" alt="{{img.alt}}">
           </div>
        </div>
        <div class="col-md-7 offset-md-1 d-flex align-items-center">

          <div class="join-content-wrapper">

          <h2>{{ section.settings.h2 }}</h2>
          <p>{{  section.settings.p }}</p>
          <div class="join-btn">
            <a href="{{ section.settings.url }}" class="btnn d-flex ">  
             <p>{{ section.settings.urlt }}</p>  
              <div class="join-svg">
                <svg xmlns="http://www.w3.org/2000/svg" width="18" height="11" viewBox="0 0 18 11" fill="none">
                  <path d="M1.5 4.5C0.947715 4.5 0.499999 4.94772 0.499999 5.5C0.499999 6.05228 0.947715 6.5 1.5 6.5V4.5ZM16.5 6.5C17.0523 6.5 17.5 6.05228 17.5 5.5C17.5 4.94772 17.0523 4.5 16.5 4.5V6.5ZM1.5 6.5L16.5 6.5V4.5L1.5 4.5V6.5Z" fill="black"/>
                  <path d="M11.8858 0.535901C11.6295 0.0466966 11.0251 -0.142098 10.5359 0.114216C10.0467 0.370531 9.8579 0.974894 10.1142 1.4641L11.8858 0.535901ZM16.1235 6.42642C16.6352 6.63435 17.2185 6.38814 17.4264 5.8765C17.6344 5.36485 17.3881 4.78151 16.8765 4.57358L16.1235 6.42642ZM10.1142 1.4641C10.7734 2.7222 11.4793 3.66431 12.4617 4.4527C13.4246 5.22542 14.6016 5.80791 16.1235 6.42642L16.8765 4.57358C15.4037 3.97504 14.4448 3.47977 13.7135 2.89287C13.0017 2.32166 12.455 1.62229 11.8858 0.535901L10.1142 1.4641Z" fill="black"/>
                  <path d="M11.8858 10.4641C11.6295 10.9533 11.0251 11.1421 10.5359 10.8858C10.0467 10.6295 9.8579 10.0251 10.1142 9.5359L11.8858 10.4641ZM16.1235 4.57358C16.6352 4.36565 17.2185 4.61186 17.4264 5.1235C17.6344 5.63515 17.3881 6.21849 16.8765 6.42642L16.1235 4.57358ZM10.1142 9.5359C10.7734 8.2778 11.4793 7.33569 12.4617 6.5473C13.4246 5.77458 14.6016 5.19209 16.1235 4.57358L16.8765 6.42642C15.4037 7.02496 14.4448 7.52023 13.7135 8.10713C13.0017 8.67834 12.455 9.37771 11.8858 10.4641L10.1142 9.5359Z" fill="black"/>
                  </svg>
              </div>  
            </a>
          </div>
          </div>
        
        </div>
      </div>
    </div>
  </div>


  {% schema %}
    {
        "name": "Home Join",
        "class": "join",
        "settings": [
            {
                "type": "image_picker",
                "id": "img",
                "label": "Please Select your Image"
            },
            {
                "type": "text",
                "id": "h2",
                "label": "Section Title",
                "default": "JOIN THE HUSTLE.",
                "placeholder": "JOIN THE HUSTLE."
            },{
                "type": "text",
                "id": "p",
                "label": "Description",
                "default": "WE ARE CHIMMI, short for 'Children of Immigrants,' is inspired by our backgrounds. We are an accessories brand that donates 10% of profits from sales to organizations that support immigrant and marginalized communities. Learn more about us",
        
                "placeholder": "WE ARE CHIMMI, short for 'Children of Immigrants, ........"
            },
            {
                "type": "url",
                "id": "url",
                "label": "Learn More Link"
            },
            {
                "type": "text",
                "id": "urlt",
                "label": "Button Name",
                "default": "Learn More",
                "placeholder": "Learn More"
            }
        
        ],"presets": [
            {
              "name": "Join With Us"
            }]
    }
  {% endschema %}
  

