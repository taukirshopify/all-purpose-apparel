                  {% elsif section.settings.arroa == "Yellow" %}
                  stroke="#F2E085"
         
                  {% endif %} stroke-width="3" stroke-miterlimit="10" stroke-linecap="round"/>
                  </svg>
              </div>
          </div>
          <div class="col-lg-4 col-md-6 col-sm-8 col-8">
            <div class="section-title text-center">
               <h2>{{ section.settings.h2 }}</h2>
            </div>
          </div>

          <!-- both are the same wave  -->
          <div class="col-lg-4  col-md-3 col-sm-2 col-2  d-flex align-items-center justify-content-end p-0">
            <div class="right-wave-rapper common-margin-common">
              <svg xmlns="http://www.w3.org/2000/svg" width="271" height="22" viewBox="0 0 271 22" fill="none">
                <path d="M2 2C15.3547 2 15.3547 20 28.7094 20C42.0641 20 42.0641 2 55.4188 2C68.7735 2 68.7735 20 82.0969 20C95.452 20 95.452 2 108.775 2C122.13 2 122.13 20 135.453 20C148.808 20 148.808 2 162.162 2C175.517 2 175.517 20 188.872 20C202.226 20 202.226 2 215.581 2C228.936 2 228.936 20 242.291 20C255.645 20 255.645 2 269 2"   {% if section.settings.arroa == "Orange" %}
                  stroke="#D77628"
                {% elsif section.settings.arroa == "Yellow" %}
                stroke="#F2E085"
       
                {% endif %} stroke-width="3" stroke-miterlimit="10" stroke-linecap="round"/>
                </svg>


  
            </div>
          </div>
      </div>
   <!-- Headign wit hway  -->

    </div>
