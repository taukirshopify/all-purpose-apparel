  {% schema %}
    {
        "name": "JEWELRY",
        "class": "new",
        "settings": [
           {
            "type": "text",
            "id": "h2",
            "label": "JEWELRY Product Collection",
            "default": "JEWELRY",
            
            "placeholder": "JEWELRY"
           },
           {
            "type": "color",
            "id": "color",
            "label": "Please select the Background Color",
            "default": "#fff"
           },
           {
            "type": "select",
            "id": "arroa",
            "label": "Please Select your Wave Color",
            
            "options": [
                {
            
                "value": "Orange",
                "label": "Orange"
                },
                {
            
                "value": "Yellow",
                "label": "Yellow"
                }
            ],
            "default": "Orange"
          
           },
           {
            "type": "collection",
            "id": "product",
            "label": "Seclect your Product Collection",
            "info": "Men's collection"
           }
        ],"presets": [
            {
              "name": "JEWELRY"
            }]

    }
  {% endschema %}
  
<style>

  .common-product-wrapper-{{section.id}} {
   
    background: {{ section.settings.color }};
}

</style>



  <script defer async>
  
    window.addEventListener('load', function () {
  

    new Splide("#first-slider-{{section.id}}",{
      type: 'loop',
      pagination : false,
      drag   : 'free',
      cover      : true,
      gap       : 00,
      perMove:1,
      focus  : 'right',
      interval: 4000,
       autoScroll: {
        speed: 1,
      },
      breakpoints : {
  
        '600': {
          perPage: 1,
          gap       : 00,
          autoScroll: {
            speed: 0.5,
          },
        },
        '960': {
          perPage: 2,
          gap       : 00,
          autoScroll: {
            speed: 0.5,
          },
        },
        '1020': {
          perPage: 3,
          gap       : 00,
          autoScroll: {
            speed: 0.5,
          },
        },
        '1333': {
          perPage: 4,
          gap       : 20,
          autoScroll: {
            speed: 0.5,
          },
        },
        '1500': {
          perPage: 4,
          gap       : 20,
          autoScroll: {
            speed: 0.5,
          },
        },
        '1920': {
          perPage: 5,
          gap       : 20,
          autoScroll: {
            speed: 1.5,
          },
        }
    },
    }).mount( window.splide.Extensions);
  })
  </script>

  {% comment %} window.splide.Extensions {% endcomment %}